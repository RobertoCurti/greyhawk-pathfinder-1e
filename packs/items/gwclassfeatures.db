{"name":"Purity of Body (MNK)","type":"feat","img":"systems/pf1/icons/skills/blue_35.jpg","flags":{"core":{"sourceId":"Compendium.pf1.class-abilities.82G7gl4C1AvtSHI4"},"pf1":{},"scene-packer":{"hash":"7d99de64b884d3df97782b542507938657a75e99","sourceId":"Item.ifWbBdmatVDZHKpV"}},"system":{"description":{"value":"<p>At 5th level, a monk gains&nbsp;<a href=\"https://www.d20pfsrd.com/bestiary/rules-for-monsters/universal-monster-rules#TOC-Immunity-Ex-or-Su-\">immunity</a>&nbsp;to all diseases, including&nbsp;<a href=\"https://www.d20pfsrd.com/magic#TOC-Supernatural-Abilities-Su-\">supernatural</a>&nbsp;and magical diseases.</p>","chat":"","unidentified":""},"tags":[],"actions":[],"uses":{"per":"","value":0,"maxFormula":""},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"classFeat_purityOfBody","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{"source":"Pathfinder Unchained pg. 14","source_type":"other"}},"scriptCalls":[],"featType":"classFeat","associations":{"classes":[["Monk"]]},"showInQuickbar":false,"abilityType":"ex","crOffset":"","disabled":false,"classSkills":{"acr":false,"apr":false,"art":false,"blf":false,"clm":false,"crf":false,"dip":false,"dev":false,"dis":false,"esc":false,"fly":false,"han":false,"hea":false,"int":false,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,"kna":false,"kno":false,"kpl":false,"kre":false,"lin":false,"lor":false,"per":false,"prf":false,"pro":false,"rid":false,"sen":false,"slt":false,"spl":false,"ste":false,"sur":false,"swm":false,"umd":false},"identifiedName":"Purity of Body (MNK)"},"effects":[],"ownership":{"default":0,"mBowgFU95wMOchUO":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","cr