{"name":"Timeless Body (MNK)","type":"feat","img":"systems/pf1/icons/skills/green_15.jpg","flags":{"core":{"sourceId":"Compendium.pf1.class-abilities.5JlthJkVGEHPZypG"},"pf1":{},"scene-packer":{"hash":"6ecea5aefd699938cc0ac62186a1d8ef0d6d6d1a","sourceId":"Item.9PQuzJMBkk6ybOOp"}},"system":{"description":{"value":"<p>This character no longer takes ability score penalties for aging and cannot be magically aged. Any penalties she may have already incurred, however, remain in place. Bonuses still accrue, and the character still dies of old age when her time is up.</p>","chat":"","unidentified":""},"tags":[],"actions":[],"uses":{"per":"","value":0,"maxFormula":""},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"classFeat_timelessBody","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{"source":"Pathfinder Unchained pg. 14","source_type":"other"}},"scriptCalls":[],"featType":"classFeat","associations":{"classes":[["Druid"],["Monk"]]},"showInQuickbar":false,"abilityType":"ex","crOffset":"","disabled":false,"classSkills":{"acr":false,"apr":false,"art":false,"blf":false,"clm":false,"crf":false,"dip":false,"dev":false,"dis":false,"esc":false,"fly":false,"han":false,"hea":false,"int":false,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,"kna":false,"kno":false,"kpl":false,"kre":false,"lin":false,"lor":false,"per":false,"prf":false,"pro":false,"rid":false,"sen":false,"slt":false,"spl":false,"ste":false,"sur":false,"swm":false,"umd":false},"identifiedName":"Timeless Body (MNK)"},"effects":[],"ownership":{"default":0,"mBowgFU95wMOchUO":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1683697710056,"modifiedTime":1684595197856,