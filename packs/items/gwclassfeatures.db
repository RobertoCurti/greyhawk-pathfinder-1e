{"name":"Still Mind (MNK)","type":"feat","img":"systems/pf1/icons/feats/blind-fight.jpg","system":{"description":{"value":"<p>At 4th level, a monk gains a +2 bonus on saving throws against enchantment spells and effects.</p>","chat":"","unidentified":""},"tags":[],"actions":[],"uses":{"per":"","value":0,"maxFormula":""},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[{"text":"+2 bonus on saving throws against enchantment spells and effects.","subTarget":"will"}],"links":{"children":[],"charges":[]},"tag":"classFeat_stillMindUmk","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{"source":"Pathfinder Unchained pg. 14","source_type":"other"}},"scriptCalls":[],"featType":"classFeat","associations":{"classes":[["Monk (Unchained)"]]},"showInQuickbar":false,"abilityType":"ex","crOffset":"","disabled":false,"classSkills":{"acr":false,"apr":false,"art":false,"blf":false,"clm":false,"crf":false,"dip":false,"dev":false,"dis":false,"esc":false,"fly":false,"han":false,"hea":false,"int":false,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,"kna":false,"kno":false,"kpl":false,"kre":false,"lin":false,"lor":false,"per":false,"prf":false,"pro":false,"rid":false,"sen":false,"slt":false,"spl":false,"ste":false,"sur":false,"swm":false,"umd":false},"identifiedName":"Still Mind (MNK)"},"flags":{"core":{"sourceId":"Compendium.pf1.class-abilities.TTn7hey3SfqaTsLr"},"pf1":{},"scene-packer":{"hash":"a19b0f9cd3ef1a77b456150d9424df2ea39d9b76","sourceId":"Item.RrizMbeH67H40RDb"}},"effects":[],"ownership":{"default":0,"mBowgFU95wMOchUO":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1683697710055,"modifiedTime":1684595177423,"lastModifiedBy":"mBowgFU95wMOchUO"},"folder":null,"sort":0,"