f (!actors.length) actors = game.actors.filter(o => o.isPC && o.testUserPermission(game.user, \"OWNER\"));\nactors = actors.filter(o => o.testUserPermission(game.user, \"OWNER\"));\n\nif (!actors.length) ui.notifications.warn(\"No applicable actor(s) found\");\nelse {\n  const _roll = async function(type) {\n    let madeSound = false;\n    for (let a = 0;a < actors.length; a++) {\n      const o = actors[a];\n      const info = o.getSkillInfo(type);\n      if (!info) continue;\n      const rollData = o.getRollData(); // Get additional roll data if needed\n      await o.rollSkill(type, { event: new MouseEvent({}), skipDialog: true, noSound: madeSound, rollData });\n      madeSound = false;\n    }\n  };\n\n  for (const s of c.skills) {\n    let info;\n    for (const o of actors) {\n      info = o.getSkillInfo(s);\n      if (info) break;\n    }\n    if (!info) continue;\n    const label = info.name;\n    _roll(s);\n  }\n}\n","category":"use","hidden":false}],"masterwork":false,"enh":null,"proficient":true,"held":"normal","showInQuickbar":true,"broken":false,"ammoType":"","attackType":"misc","weaponGroups":{"value":[],"custom":""},"identifiedName":"Acrobatics"},"sort":-10,"_id":"3o6oc7ZlaxjHb92I","img":"systems/pf1/icons/feats/skill-focus.jpg","effects":[],"folder":null,"ownership":{"default":0,"mBowgFU95wMOchUO":3},"flags":{"scene-packer":{"hash":"2598bb536eaea2b47e416375cc8cfcb2b00d0592"}},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1684382086483,"modifiedTime":1684382438482,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"name":"Darkvision, 60ft [+2RP]","type":"feat","img":"modules/greyhawk-pathfinder-1e/images/icons/unlabeled/purple-015.png","system":{"description":{"value":"<p><span style=\"font-family: Signika, sans-serif\"><strong style=\"box-sizing: border-box; user-select: text;\">Race Points</strong><br style=\"box-sizing: border-box; user-select: text;\">2 point(s)</span></p><p style=\"box-sizing: border-box; user-select: te