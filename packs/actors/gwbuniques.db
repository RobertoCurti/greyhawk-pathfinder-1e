{"name":"Malhazar, the Exiled Flame","type":"npc","img":"modules/greyhawk-pathfinder-1e/images/antagonists/MalhazarPort.png","items":[{"_id":"c0nvzuWq2aSnmjdF","name":"Genie, Efreeti","type":"race","img":"icons/creatures/magical/spirit-undead-horned-blue.webp","effects":[],"folder":null,"sort":0,"flags":{},"system":{"description":{"value":"","chat":"","unidentified":""},"tags":[],"changes":[{"_id":"6kxhenxm","formula":"10","operator":"add","subTarget":"str","modifier":"racial","priority":0,"value":10},{"_id":"7azgezqd","formula":"6","operator":"add","subTarget":"dex","modifier":"racial","priority":0,"value":6},{"_id":"f7in8gfd","formula":"8","operator":"add","subTarget":"con","modifier":"racial","priority":0,"value":8},{"_id":"r968zwgn","formula":"2","operator":"add","subTarget":"int","modifier":"racial","priority":0,"value":2},{"_id":"f0mttf5c","formula":"4","operator":"add","subTarget":"wis","modifier":"racial","priority":0,"value":4},{"_id":"c91xaoxp","formula":"4","operator":"add","subTarget":"cha","modifier":"racial","priority":0,"value":4},{"_id":"7rkxcwyu","formula":"8","operator":"add","subTarget":"nac","modifier":"base","priority":0,"value":8},{"_id":"ah3cfz9w","formula":"1","operator":"add","subTarget":"bonusFeats","modifier":"untyped","priority":0,"value":1}],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[]},"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"creatureType":"outsider","size":"med","subTypes":[["Extraplanar"],["Fire"]],"conditionals":[],"identifiedName":"Genie, Efreeti","classSkills":{"acr":false,"apr":false,"art":false,"blf":false,"clm":false,"crf":false,"dip":false,"dev":false,"dis":false,"esc":false,"fly":false,"han":false,"hea":false,"int":false,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,