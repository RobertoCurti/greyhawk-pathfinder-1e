Attacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":"23","type":"","description":""},"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"size":"med","identifiedName":"Unholy Aura"},"ownership":{"default":0},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":null,"modifiedTime":1684130696414,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"Zeq6RWYv5otvg8q7","name":"Cleave","type":"feat","img":"systems/pf1/icons/feats/cleave.jpg","effects":[],"folder":null,"sort":300000,"flags":{},"system":{"description":{"value":"<em>You can strike two adjacent foes with a single swing.</em><br/><br/><b>Prerequisites</b><br/>Str 13, Power Attack, base attack bonus +1.<br/><br/><b>Benefits</b><br/>As a standard action, you can make a single attack at your full base attack bonus against a foe within reach. If you hit, you deal damage normally and can make an additional attack (using your full base attack bonus) against a foe that is adjacent to the first and also within reach. You can only make one additional attack per round with this feat. When you use this feat, you take a -2 penalty to your Armor Class until your next turn.<br/><br/><b>Combat Trick</b><br/>When using the Cleave or Great Cleave feat, you can spend 4 stamina points to negate the -2 penalty to Armor Class until your next turn.","chat":"","unidentified":""},"tags":[["PFS"],["Combat"]],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","autoDeductCharges":true,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[{"subTarget":"attack","text":"Cleave: if you hit, you deal damage normally and can make an additional attack (using your full base attack bonus) against a foe that is adjacent to the first and also within reach."},{"subTarget":"ac","text":"Cleave: [[-2]] penalty until your next turn."}],"links":{"children":[],"charges":[]},"tag":"feat_cleave","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"nonlethal":false,"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"size":"med"},"ownership":{"default":0},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":null,"modifiedTime":1684129084388,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"dn2KgHFQRbuoNRx0","name":"Improved Bull Rush","type":"feat","img":"systems/pf1/icons/feats/improved-bull-rush.jpg","effects":[],"folder":null,"sort":400000,"flags":{},"system":{"description":{"value":"\n                <i>You are skilled at pushing your foes around.</i>\n                <br />\n                <br />\n                \n                <p><b>Prerequisites</b></p>\n                <p> Str 13, Power Attack, base attack bonus +1.</p>\n                \n            <br />\n                <p><b>Benefits</b></p>\n                <p><p class=\"stat-block-1\"> You do not provoke an attack of opportunity when performing a bull rush combat maneuver. In addition, you receive a +2 bonus on checks made to bull rush a foe. You also receive a +2 bonus to your Combat Maneuver Defense whenever an opponent tries to bull rush you.</p></p>\n                \n            <br />\n                <p><b>Normal</b></p>\n                <p><p class=\"stat-block-1\"> You provoke an attack of opportunity when performing a bull rush combat maneuver.</p></p>\n                \n            \n            ","chat":"","unidentified":""},"tags":[["Combat"],["Combat Maneuver"],["Offensive"]],"actions":[],"uses":{"per":null,"value":0,"maxFormula":"","autoDeductCharges":true,"autoDeductChargesCost":"1"},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"feat_improvedBullRush","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{},"nonlethal":false,"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"measureTemplate":{"type":"","size":"0","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label