ar"],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"creatureType":"outsider","size":"med","subTypes":[["asura"],["evil"],["extraplanar"],["lawful"]],"classSkills":{"acr":true,"apr":false,"art":false,"blf":true,"clm":false,"crf":true,"dip":true,"dev":false,"dis":false,"esc":true,"fly":false,"han":false,"hea":false,"int":true,"kar":false,"kdu":false,"ken":false,"kge":false,"khi":false,"klo":false,"kna":false,"kno":false,"kpl":true,"kre":false,"lin":false,"lor":false,"per":false,"prf":false,"pro":false,"rid":false,"sen":true,"slt":false,"spl":false,"ste":true,"sur":false,"swm":false,"umd":false},"identifiedName":"Upasunda"},"effects":[],"folder":null,"sort":0,"ownership":{"default":0,"mBowgFU95wMOchUO":3},"flags":{"scene-packer":{"hash":"4a1fb68fc43cd34c95bcf05572072338b1880f38"}},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1684141473383,"modifiedTime":1684146287746,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"hTwuekYdy9z2ATEI","name":"Power Attack","type":"feat","img":"systems/pf1/icons/feats/power-attack.jpg","sort":400000,"flags":{"core":{"sourceId":"Compendium.pf1.feats.FUW5mIXHNBBIQ1Sq"},"scene-packer":{"hash":"3a1063bb30f485417eedbb2289035384035497de"}},"system":{"description":{"value":"\n                <i>You can make exceptionally deadly melee attacks by sacrificing accuracy for strength.</i>\n                <br />\n                <br />\n                \n                <p><b>Prerequisites</b></p>\n                <p> Str 13, base attack bonus +1.</p>\n                \n            <br />\n                <p><b>Benefits</b></p>\n                <p><p class=\"stat-block-1\"> You can choose to take a &ndash;1 penalty on all melee attack rolls and combat maneuver checks to gain a +2 bonus on all melee damage rolls. This bonus to damage is increased by half (+50%) if you are making an attack with a two-handed weapon, a one handed weapon using two hands, or a primary natural weapon that adds 1-1/2 times your Strength modifier on damage rolls. This bonus to damage is halved (&ndash;50%) if you are making an attack with an off-hand weapon or secondary natural weapon. When your base attack bonus reaches +4, and every 4 points thereafter, the penalty increases by &ndash;1 and the bonus to damage increases by +2. You must choose to use this feat before making an attack roll, and its effects last until your next turn. The bonus damage does not apply to touch attacks or effects that do not deal hit point damage.</p></p>\n                \n            \n            ","chat":"","unidentified":""},"tags":[["Combat"],["Offensive"]],"actions":[],"uses":{"per":null,"value":0,"maxFormula":""},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"feat_powerAttack","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{}},"effects":[],"folder":null,"ownership":{"default":0,"mBowgFU95wMOchUO":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1684144223799,"modifiedTime":1684144311675,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"MmE5PvpmiMA22sLJ","name":"Spring Attack","type":"feat","img":"systems/pf1/icons/feats/spring-attack.jpg","sort":500000,"flags":{"core":{"sourceId":"Compendium.pf1.feats.ugQyUpqPBDsqLyra"},"scene-packer":{"hash":"03dd55301d3ea2fdabdcd65a8bc724d9511c16dd"}},"system":{"description":{"value":"\n                <i>You can deftly move up to a foe, strike, and withdraw before he can react.</i>\n                <br />\n                <br />\n                \n                <p><b>Prerequisites</b></p>\n                <p> Dex 13, Dodge, Mobility, base attack bonus +4.</p>\n                \n            <br />\n                <p><b>Benefits</b></p>\n                <p><p class=\"stat-block-1\"> As a full-round action, you can move up to your speed and make a single melee attack without provoking any attacks of opportunity from the target of your attack. You can move both before and after the attack, but you must move at least 10 feet before the attack and the total distance that you move cannot be greater than your speed. You cannot use this ability to attack a foe that is adjacent to you at the start of your turn.</p></p>\n                \n            <br />\n                <p><b>Normal</b></p>\n                <p><p class=\"stat-block-1\"> You cannot move before and after an attack.</p></p>\n                \n            \n            ","chat":"","unidentified":""},"tags":[["Combat"],["Offensive"],["Movement"]],"actions":[],"uses":{"per":null,"value":0,"maxFormula":""},"attackNotes":[],"effectNotes":[],"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"feat_springAttack","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"languages":{"value":[],"custom":""},"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"featType":"feat","associations":{"classes":[]},"showInQuickbar":false,"abilityType":"none","crOffset":"","disabled":false,"classSkills":{}},"effects":[],"folder":null,"ownership":{"default":0,"mBowgFU95wMOchUO":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1684144239809,"modifiedTime":1684144311675,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"wK016XSeaUsvrXKc","name":"Combat Reflexes","type":"feat","img":"systems/pf1/icons/feats/combat-reflexes.jpg","sort":200000,"flags":{"core":{"sourceId":"Compendium.pf1.feats.h9nHYLxXvIXBTmup"},"scene-packer":{"hash":"0a1f12b9bad7a6f7c1da12432456224ce8463cfd"}},"system":{"description":{"value":"<p><em>You can make additional attacks of opportunity.</em><br /><br /><strong>Benefits</strong><br />You may make a number of additional attacks of opportunity per round equal to your Dexterity bonus. With this feat, you may also make attacks of opportunity while flat-footed.<br /><br /><strong>Normal</strong><br />A character without this feat can make only one attack of opportunity per round and can't make attacks of opportunity while flat-footed.<br /><br /><strong>Special</strong><br />The Combat Reflexes feat does not allow a rogue to use her opportunist ability more than once per round.<br /><br /><strong>Combat Trick</strong><br />When you miss with an attack of opportunity, you can spend 5 stamina points to make a second attack for the same provoking action. That second attack of opportunity takes a -5 penalty on the attack roll and costs one of your attacks of opportunity for the round.</p>\n<p>&nbsp;</p>\n<p><strong>Usage</strong>: spend 1 charge after use.</p>","chat":"","unidentified":""},"tags":[["PFS"],["Combat"]],"actions":[{"_id":"kzof1vn44xdata40","name":"Use","img":"systems/pf1/icons/feats/combat-reflexes.jpg","description":"","tag":"","activation":{"cost":1,"type":"passive"},"unchainedAction":{"activation":{"cost":1,"type":"passive"}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"autoDeductChargesCost":"","self":{"value":0,"maxFormula":"","per":null},"per":"charges","value":1,"maxFormula":"1 + @abilities.dex.mod"},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"fo