es":[{"_id":"qrnq0w11","formula":"2 + min(3, floor(@item.level / 6))","operator":"add","subTarget":"ac","modifier":"deflection","priority":0,"value":4,"target":"ac"}],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[]},"tag":"","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"buffType":"temp","active":false,"level":12,"duration":{"value":"@item.level","units":"minute","start":408},"hideFromToken":false,"identifiedName":"Shield of Faith"},"effects":[],"folder":null,"ownership":{"default":0,"WnztbgipWMZbC6Pz":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1682433453242,"modifiedTime":1683954762382,"lastModifiedBy":"mBowgFU95wMOchUO"}},{"_id":"U06USuy5EQYfjZ64","name":"Resistance Mastery","type":"buff","img":"systems/pf1/icons/feats/spell-focus.jpg","system":{"description":{"value":"<p><em>You can protect yourself from harm by using abjuration magic items.</em><br><br><strong>Prerequisites</strong><br>Use Magic Device 3 ranks, base Fortitude saving throw bonus +3.<br><br><strong>Benefits</strong><br>Once per day, you can meditate for 10 minutes in order to use magic armor or a wondrous item that has an abjuration spell of 2nd level or higher listed in its construction requirements and that occupies a slot on your body to gain a defensive benefit. This benefit can be either a +1 resistance bonus on all saving throws or 5 points of energy resistance against one energy type (acid, cold, electricity, fire, or sonic). This benefit lasts for 24 hours. You must wear the item in order to gain this benefit; if the item is removed before this feat's benefit ends, the benefit is immediately lost and cannot be regained until 24 hours have passed since the feat's last activation. The resistance bonus granted by this feat increases by 1 or the energy resistance increases by 5 when your base Fortitude save bonus reaches +6, +9, and +12. You can gain only one benefit from this feat at a time.</p><p><br><strong>Pathfinder Society<br></strong>When qualifying for and calculating the benefits of an item mastery feat, a PC uses the base Fortitude save bonus from only one of his classes.</p>","chat":"","unidentified":""},"tags":[],"changes":[{"_id":"6q14yfli","formula":"4","operator":"add","subTarget":"allSavingThrows","modifier":"resist","priority":0,"value":4,"target":"savingThrows"}],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[]},"tag":"resistance","useCustomTag":false,"flags":{"boolean":{},"dictionary":{}},"scriptCalls":[],"buffType":"temp","active":true,"level":null,"duration":{"value":"","units":"","start":408},"hideFromToken":false,"uses":{"value":0,"max":0},"identifiedName":"Resistance Mastery","range":{"maxIncrements":1}},"ownership":{"default":0,"WnztbgipWMZbC6Pz":3},"_stats":{"systemId":"pf1","systemVersion":"0.82.5","coreVersion":"10.291","createdTime":1665428265978,"modifiedTime":1682434111476,"lastModifiedBy":"WnztbgipWMZbC6Pz"},"effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf-content.pf-buffs.GdU6Xlwn2phRnfc6"}}},{"type":"consumable","name":"Potion of Cure Serious Wounds","system":{"description":{"value":"<p>This spell functions like <i>cure light wounds</i>, except that it cures 3d8 points of damage + 1 point per caster level (maximum +15).</p>\n","chat":"","unidentified":""},"tags":[],"quantity":3,"weight":{"value":0},"price":750,"size":"med","equipped":true,"resizing":false,"identified":true,"hp":{"max":1,"value":1},"broken":false,"hardness":1,"carried":true,"unidentified":{"price":0,"name":""},"identifiedName":"Potion of Cure Serious Wounds","cl":5,"aura":{"custom":false,"school":"con"},"actions":[{"_id":"3Sz0yvYgnnp5P8O6","name":"Drink","img":"systems/pf1/icons/items/potions/minor-blue.jpg","description":"","tag":"","a